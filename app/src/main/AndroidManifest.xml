<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.uniorganizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Friendtransaction.ActivityRegister"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Stundenplan.EditTimetableActivity"
            android:label="UniOrganizer: Stundenplan"></activity>
        <activity
            android:name=".Stundenplan.MondayActivity"
            android:label="UniOrganizer: Monday"></activity>
        <activity
            android:name=".Stundenplan.TuesdayActivity"
            android:label="UniOrganizer: Tuesday "></activity>
        <activity
            android:name=".Stundenplan.WednesdayActivity"
            android:label="UniOrganizer: Wednesday"></activity>
        <activity
            android:name=".Stundenplan.ThursdayActivity"
            android:label="UniOrganizer: Thursday "></activity>
        <activity
            android:name=".Stundenplan.FridayActivity"
            android:label="UniOrganizer: Friday"></activity>
        <activity
            android:name=".Stundenplan.FriendsActivity"
            android:label="UniOrganizer: Friendlist"></activity>
    </application>

</manifest>